{
  "name": "ca-mgmt-server",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --build",
    "start": "node dist/index.js",
    "prod": "cross-env NODE_ENV=prod yarn start",
    "dev": "cross-env NODE_ENV=dev ts-node-dev --rs --watch src --notify --respawn src/index.ts",
    "eslint": "eslint src/**/*.ts",
    "pretest": "yarn eslint",
    "test": "cross-env NODE_ENV=test nyc mocha --exit -r ts-node/register test/**/*.spec.ts --timeout 100000",
    "precoverage": "yarn eslint",
    "coverage": "NODE_ENV=test nyc --reporter=lcov mocha --exit -r ts-node/register test/**/*.spec.ts --timeout 100000"
  },
  "repository": "git@github.com:ku-cylee/ca-mgmt-server.git",
  "author": "Chanyoung Lee <cylee@davinci.snu.ac.kr>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.12.3",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.4",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
